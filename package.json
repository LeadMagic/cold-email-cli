{
  "name": "cold-email-cli",
  "version": "2.0.0",
  "type": "module",
  "description": "Professional CLI for cold email automation across multiple platforms - Built by the community",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cold-email-cli": "dist/cli.js",
    "cec": "dist/cli.js"
  },
  "scripts": {
    "dev": "bun run --watch src/cli.ts",
    "build": "bun run scripts/build.ts",
    "build:fast": "npx tsc && cp package.json dist/",
    "start": "node dist/cli.js",
    "test": "bun test",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "link": "npm link",
    "package": "npm run build:fast && npm pack"
  },
  "keywords": [
    "cold-email",
    "email-automation",
    "smartlead",
    "instantly",
    "salesforge",
    "apollo",
    "cli",
    "react-ink",
    "typescript"
  ],
  "author": "Community Contributors",
  "license": "MIT",
  "homepage": "https://github.com/LeadMagic/cold-email-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeadMagic/cold-email-cli.git"
  },
  "bugs": {
    "url": "https://github.com/LeadMagic/cold-email-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ink": "^4.4.1",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "react": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "community": {
    "discord": "https://discord.gg/mB76X5QJ"
  }
}
