{
  "name": "smartlead-cli",
  "version": "2.0.0",
  "description": "ðŸš€ The most powerful and beautiful command-line interface for SmartLead API management. Features responsive design, full-screen shell mode, advanced filtering, and comprehensive campaign management.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "smartlead": "dist/index.js",
    "sl": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "package": "npm run clean && npm run build && npm pack",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "install-global": "npm run build && npm install -g .",
    "uninstall-global": "npm uninstall -g smartlead-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/smartlead-cli.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/smartlead-cli/issues"
  },
  "homepage": "https://smartlead-cli.com",
  "keywords": [
    "smartlead",
    "cli",
    "email",
    "campaign",
    "marketing",
    "api",
    "terminal",
    "command-line",
    "typescript",
    "interactive",
    "full-screen",
    "shell",
    "leads",
    "analytics",
    "warmup",
    "filtering",
    "responsive",
    "beautiful",
    "enterprise"
  ],
  "author": {
    "name": "SmartLead CLI Team",
    "email": "team@smartlead-cli.com",
    "url": "https://smartlead-cli.com"
  },
  "contributors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "gradient-string": "^1.2.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.3",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "@types/node": "^20.10.4",
    "@types/inquirer": "^8.2.10",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "node": ">=16.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/smartlead-cli"
  }
}
