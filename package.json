{
  "name": "professional-cold-email-cli",
  "version": "2.0.0",
  "description": "ðŸš€ Professional multi-platform CLI for cold email outreach automation. Complete TypeScript implementation with SmartLead, Instantly, and SalesForge modules, 200+ commands, beautiful UI, and comprehensive testing. Sponsored by LeadMagic.",
  "main": "dist/core/index.js",
  "types": "dist/core/index.d.ts",
  "bin": {
    "cold-email-cli": "dist/bin/smartlead",
    "ce": "dist/bin/sl"
  },
  "files": [
    "dist/**/*",
    "docs/README.md",
    "docs/CONTRIBUTING.md",
    "docs/ROADMAP.md",
    "docs/CHANGELOG.md",
    "LICENSE",
    "scripts/install.sh",
    "scripts/install-mac.sh",
    "build/**/*"
  ],
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:mac": "bash scripts/build-mac.sh",
    "build:installer": "bash scripts/create-mac-installer.sh",
    "dev": "ts-node --project tsconfig.json src/core/index.ts",
    "watch": "nodemon --exec ts-node --project tsconfig.json src/core/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage build",
    "prepare": "npm run build",
    "prepack": "npm run build",
    "install-global": "npm run build && npm install -g .",
    "uninstall-global": "npm uninstall -g professional-cold-email-cli",
    "setup": "bash scripts/install.sh",
    "setup:mac": "bash scripts/install-mac.sh",
    "package": "npm pack",
    "package:mac": "npm run build:installer",
    "start": "node dist/core/index.js",
    "release": "npm run build && npm run package:mac"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeadMagic/cold-email-cli.git"
  },
  "bugs": {
    "url": "https://github.com/LeadMagic/cold-email-cli/issues"
  },
  "homepage": "https://github.com/LeadMagic/cold-email-cli#readme",
  "keywords": [
    "cold-email",
    "email-outreach",
    "sales-automation",
    "smartlead",
    "instantly",
    "salesforge",
    "cli",
    "campaign",
    "lead-generation",
    "api",
    "terminal",
    "command-line",
    "typescript",
    "modular",
    "automation",
    "crm",
    "outreach",
    "sales",
    "nodejs",
    "multi-platform",
    "professional",
    "enterprise",
    "b2b",
    "prospecting",
    "email-marketing",
    "leadmagic",
    "macos",
    "installer"
  ],
  "author": {
    "name": "LeadMagic",
    "email": "support@leadmagic.io",
    "url": "https://leadmagic.io"
  },
  "contributors": [
    {
      "name": "Cold Email CLI Contributors",
      "url": "https://github.com/LeadMagic/cold-email-cli/graphs/contributors"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "gradient-string": "^1.2.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/gradient-string": "1.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "node": ">=16.0.0"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/LeadMagic"
    },
    {
      "type": "company",
      "url": "https://leadmagic.io/sponsor"
    }
  ],
  "sponsor": {
    "name": "LeadMagic",
    "url": "https://leadmagic.io",
    "logo": "https://leadmagic.io/assets/logo.png",
    "description": "Advanced B2B lead generation and cold email automation platform"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
