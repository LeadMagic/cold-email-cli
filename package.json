{
  "name": "cold-email-cli",
  "version": "2.0.0",
  "type": "module",
  "description": "Professional CLI for cold email automation across multiple platforms - Built by the community",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cold-email-cli": "dist/cli.js",
    "cec": "dist/cli.js"
  },
  "packageManager": "bun@1.0.0",
  "scripts": {
    "dev": "bun run --watch src/cli.ts",
    "build": "bun run scripts/build.ts",
    "build:fast": "bun x tsc && cp package.json dist/",
    "build:binary": "bun run scripts/build.ts",
    "start": "bun dist/cli.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "bun x tsc --noEmit",
    "lint": "bun x biome check src/",
    "lint:fix": "bun x biome check --write src/",
    "format": "bun x biome format --write src/",
    "format:check": "bun x biome format src/",
    "quality": "bun run type-check && bun run lint && bun run format:check",
    "quality:fix": "bun run type-check && bun run lint:fix && bun run format",
    "clean": "rm -rf dist build/*.app",
    "clean:all": "rm -rf dist build node_modules .bun package-lock.json",
    "link": "bun link",
    "package": "bun run build:fast && bun run pack",
    "install:global": "bun install -g .",
    "dev:platforms": "bun run --watch src/cli.ts platforms"
  },
  "keywords": [
    "cold-email",
    "email-automation",
    "smartlead",
    "instantly",
    "salesforge",
    "apollo",
    "emailbison",
    "amplemarket",
    "lemlist",
    "outreach",
    "salesloft",
    "cli",
    "react-ink",
    "typescript",
    "bun"
  ],
  "author": "Community Contributors",
  "license": "MIT",
  "homepage": "https://github.com/LeadMagic/cold-email-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeadMagic/cold-email-cli.git"
  },
  "bugs": {
    "url": "https://github.com/LeadMagic/cold-email-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ink": "^4.4.1",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "react": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "community": {
    "discord": "https://discord.gg/mB76X5QJ"
  },
  "funding": null
}
